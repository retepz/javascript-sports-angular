{
  "name": "javascript-sports-angular",
  "version": "0.0.0",
  "scripts": {
    "cp:local": "shx cp ./src/environments/local/environment.ts ./src/environment.ts",
    "cp:prod": "shx cp ./src/environments/production/environment.ts ./src/environment.ts",
    "ng": "ng",
    "start": "npm run cp:local && ng serve",
    "start:prod": "npm run cp:prod && ng serve",
    "build": "npm run cp:local && ng build",
    "build:prod": "npm run cp:prod && ng build",
    "watch": "npm run cp:local && ng build --watch --configuration development",
    "test": "npm run cp:local && ng test"
  },
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@tanstack/angular-query-experimental": "^5.75.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "angular-eslint": "^19.3.0",
    "@angular-devkit/build-angular": "^19.2.10",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.0",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@types/jasmine": "~5.1.0",
    "eslint": "^9.25.1",
    "eslint-plugin-prettier":"^5.2.6",
    "eslint-config-prettier": "^10.1.2",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "shx": "^0.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.31.0"
  }
}
